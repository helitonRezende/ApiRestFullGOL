<script src="~/Scripts/ControleAplicacao/Airplane.js"></script>

<div>
    <h3 style="color:red">Airplane</h3>

    <div ng-controller="Airplane">

        <div ng-init="LoadAirplane()"></div>

        <table class="table table-striped table-responsive table-condensed">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Modelo</th>
                    <th>Qtde.Passageiros</th>
                    <th>Dt.Criação</th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="item in lista">
                    <td><strong>{{ item.ID }}</strong></td>
                    <td><strong>{{ item.MODELO }}</strong></td>
                    <td><strong>{{ item.QTDEPASSAGEIRO }}</strong></td>
                    <td><strong>{{ item.DATACRIACAO }}</strong></td>
                </tr>
            </tbody>
        </table>

    </div>
</div>